module learnyou.chapter13.ReaderUghNotThisJokeAgain where

addStuff :: Int -> Int
addStuff = do
    a <- (* 2)
    b <- (+ 10)
    return (a + b)

addStuff' :: Int -> Int
addStuff' x =
    let a = (* 2) x
        b = (+ 10) x
    in  a + b

main _ = do
    println $ addStuff 3
    println $ addStuff' 3
